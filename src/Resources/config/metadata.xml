<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
	<services>
		

   <!--discover -->
   <service id="Olla\Prisma\Discover\Admin" class="Olla\Prisma\Discover\Admin" >
     <argument type="service" id="annotation_reader" />
     <argument type="service" id="serializer" />
     <argument type="service" id="property_info" />
   </service>
   <service id="Olla\Prisma\Discover\Frontend" class="Olla\Prisma\Discover\Frontend" >
     <argument type="service" id="annotation_reader" />
     <argument type="service" id="serializer" />
     <argument type="service" id="property_info" />
   </service>
   <service id="Olla\Prisma\Discover\Operation" class="Olla\Prisma\Discover\Operation" >
     <argument type="service" id="annotation_reader" />
     <argument type="service" id="serializer" />
     <argument type="service" id="property_info" />
   </service>
   <service id="Olla\Prisma\Discover\Resource" class="Olla\Prisma\Discover\Resource" >
     <argument type="service" id="annotation_reader" />
     <argument type="service" id="serializer" />
     <argument type="service" id="property_info" />
   </service>
 

   <!--Factory-->
   <service id="Olla\Prisma\Factory\Operation" class="Olla\Prisma\Factory\Operation" >

   </service>
   <service id="Olla\Prisma\Factory\Resource" class="Olla\Prisma\Factory\Resource" >

   </service>
 
 

   <!--Command-->
   <service id="Olla\Prisma\Command\MetadataCommand" class="Olla\Prisma\Command\MetadataCommand" >
     <argument type="service" id="Olla\Prisma\Metadata" />
     <tag name="console.command"  />
   </service>
 </services>
</container>