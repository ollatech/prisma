<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
	<services>
    <service id="Olla\Prisma\Placeholder\Controller" class="Olla\Prisma\Placeholder\Controller" />
    <service id="Olla\Prisma\Placeholder\Operation" class="Olla\Prisma\Placeholder\Operation" />
   
    <service id="Olla\Prisma\Route\AdminRoute" class="Olla\Prisma\Route\AdminRoute" >
      <argument type="service" id="Olla\Prisma\Metadata" />
      <tag name="routing.loader" />
    </service>
    <service id="Olla\Prisma\Route\ApiRoute" class="Olla\Prisma\Route\ApiRoute" >
      <argument type="service" id="Olla\Prisma\Metadata" />
      <tag name="routing.loader" />
    </service>
    <service id="Olla\Prisma\Route\FrontendRoute" class="Olla\Prisma\Route\FrontendRoute" >
      <argument type="service" id="Olla\Prisma\Metadata" />
      <tag name="routing.loader" />
    </service>
    <service id="Olla\Prisma\Route\GraphqlRoute" class="Olla\Prisma\Route\GraphqlRoute" >
      <argument type="service" id="Olla\Prisma\Metadata" />
      <tag name="routing.loader" />
    </service>
 </services>
</container>