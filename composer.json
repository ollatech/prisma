{
    "name": "olla/prisma",
    "type": "library",
    "description": "Symfony development framework",
    "keywords": ["REST", "GRAPHQL"],
    "homepage": "https://olla.tech/prisma",
    "license": "MIT",
    "authors": [
        {
            "name": "Olla Tech",
            "email": "contact@olla.tech",
            "homepage": "https://olla.tech"
        }
    ],
    "require": {
        "php": ">=7.0",
        "doctrine/inflector": "^1.0",
        "psr/cache": "^1.0",
        "symfony/dependency-injection": "^3.3@dev || ^4.0@dev",
        "symfony/property-access": "^2.7 || ^3.0 || ^4.0",
        "symfony/property-info": "^3.1 || ^4.0",
        "symfony/serializer": "^3.1 || ^4.0",
        "symfony/yaml": "^3.3 || ^4.0",
        "doctrine/annotations": "^1.2",
        "olla/operation": "*"
    },
    "require-dev": {
        "olla/devtool": "*"
    },
    "conflict": {
    },
    "suggest": {
    
    },
    "autoload": {
        "psr-4": { "Olla\\Prisma\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Olla\\Prisma\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
